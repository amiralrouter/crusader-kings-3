<Window x:Class="Crusader_Kings_3.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Crusader_Kings_3" 
        xmlns:module="clr-namespace:Crusader_Kings_3.Module" 
        mc:Ignorable="d"
        Title="MainWindow" Height="588" Width="1179" 
        >
    <Grid Background="#f1f5f9"> 
        <TabControl x:Name="tabControl" Grid.ColumnSpan="2">
            <TabItem Header="Main Player" >
                <module:ModulePlayer x:Name="MainPlayer"/>
            </TabItem>
            <TabItem Header="Selected Player">
                <module:ModulePlayer  x:Name="SelectedPlayer"/>
            </TabItem>
            <TabItem Header="Faith">
            </TabItem>
            <TabItem Header="Army">
            </TabItem>
            <TabItem Header="Siege">
            </TabItem>
            <TabItem Header="Holding">
            </TabItem>
            <TabItem Header="Culture">
            </TabItem>
        </TabControl>

    </Grid>
</Window>
